  "scripts": {
    "clear": "rimraf dist",
    "build:server": "rimraf dist/server && rimraf dist/common && nest build",
    "build:client": "rimraf dist/cleant && webpack",
    "format": "prettier --write \"src/**/*.ts\" \"src/server/test/**/*.ts\"",
    "start:server": "nest start",
    "start:server:dev": "nest start --watch",
    "start:server:debug": "nest start --debug --watch",
    "start:server:prod": "node dist/server/main",
    "server": "cross-env RUN_MODE=ts-node ts-node --project tsconfig.build.json ./src/server/main.ts",
    "start:client": "webpack serve",
    "start:client:prod": "echo 'ToDo'",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config src/server/test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "console:dev": "cross-env RUN_MODE=ts-node TYPEORM_HIDE_LOG=1 ts-node -r tsconfig-paths/register src/server/console.ts",
    "console": "cross-env TYPEORM_HIDE_LOG=1 node dist/server/console.js",
    "extract": "formatjs extract",
    "compile": "formatjs compile"
  },
